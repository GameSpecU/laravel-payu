{
    "name": "gamespecu/laravel-payu",
    "description": ":package_description",
    "license": "license",
    "authors": [
        {
            "name": "Dawid Focht",
            "email": "dawid@focht.pl"
        }
    ],
    "homepage": "https://github.com/gamespecu/laravel-payu",
    "keywords": ["Laravel", "LaravelPayu"],
    "require": {
        "openpayu/openpayu": "^2.3",
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Gamespecu\\LaravelPayu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gamespecu\\LaravelPayu\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gamespecu\\LaravelPayu\\LaravelPayuServiceProvider"
            ],
            "aliases": {
                "LaravelPayu": "Gamespecu\\LaravelPayu\\Facades\\LaravelPayu"
            }
        }
    }
}
